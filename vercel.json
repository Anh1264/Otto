{
    "builds": [{
        "src": "otto/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "otto/wsgi.py"
        }
    ],
    "env": {
        "MYSQLCLIENT_CFLAGS": "--cflags",
        "MYSQLCLIENT_LDFLAGS": "--libs"
    }
}